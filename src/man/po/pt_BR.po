msgid ""
"This template will add the string which is stored in the "
"uniformResourceIdentifier component of the SAN."
msgstr ""

#. type: Content of:
#. <reference><refentry><refsect1><refsect2><para><variablelist><varlistentry><listitem><para>
#: sss-certmap.5.xml:530
msgid "Example: (uri={subject_uri})"
msgstr ""

#. type: Content of:
#. <reference><refentry><refsect1><refsect2><para><variablelist><varlistentry><term>
