"id_provider = idp\n"
"idp_client_id = myclient\n"
"idp_client_secret = YOUR-CLIENT-SCERET\n"
"idp_token_endpoint = https://master.keycloak.test:8443/auth/realms/master/"
"protocol/openid-connect/token\n"
"idp_userinfo_endpoint = https://master.keycloak.test:8443/auth/realms/master/"
